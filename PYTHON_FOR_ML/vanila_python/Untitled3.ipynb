    "class Exp:\n",
    "    def __init__(self,n):\n",
    "        self.n = n\n",
    "    def __pow__(self,other):\n",
    "        return self.n**other.n\n",
    "    \n",
    "    \n",
    "    \n",
    "s = Exp(2)\n",
    "y = Exp(4)\n",
    "s**y"
